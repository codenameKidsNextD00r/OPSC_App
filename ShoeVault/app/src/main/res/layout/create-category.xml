<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ronnie_fieg_asics_free_background__iphone_desktop_"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/asap_medium"
        android:outlineProvider="none"
        android:text="Create a category"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="632dp"
        android:layout_height="421dp"
        android:scaleX="1"
        app:srcCompat="@drawable/square_rounded_xxl"
        tools:layout_editor_absoluteX="-111dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name your category:"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="99dp" />

    <Button
        android:id="@+id/btnLogin3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:backgroundTint="#4CAF50"
        android:hapticFeedbackEnabled="true"
        android:shadowColor="#FFFFFF"
        android:text="ADD CATEGORY"
        android:textColor="@color/black"
        app:cornerRadius="@android:dimen/notification_large_icon_height"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:rippleColor="#F44336"
        tools:layout_editor_absoluteY="172dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="512dp"
        android:layout_height="311dp"
        android:scaleX="3"
        app:srcCompat="@drawable/square_rounded_xxl"
        tools:layout_editor_absoluteX="-29dp"
        tools:layout_editor_absoluteY="450dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="_______________________________________________________________"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="649dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="47dp"
        android:layout_height="47dp"
        app:srcCompat="@drawable/add"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="673dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="47dp"
        android:layout_height="47dp"
        app:srcCompat="@drawable/icons8_trophy_96"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="673dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="47dp"
        android:layout_height="47dp"
        app:srcCompat="@drawable/icons8_home_96"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="673dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="46dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/icons8_search_50"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="673dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="47dp"
        android:layout_height="47dp"
        app:srcCompat="@drawable/icons8_user_96"
        tools:layout_editor_absoluteX="348dp"
        tools:layout_editor_absoluteY="673dp" />

</androidx.constraintlayout.widget.ConstraintLayout>